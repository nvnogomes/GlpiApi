{
  "Glpi": {
    "ApiUrl": "http://localhost/glpi/apirest.php/",
    "WsUsername": "",
    "WsPassword": "",
    "AppToken": ""
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\GLPIService.log",
          "rollingInterval": "Month"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
